<h1>Extensions in <%= @organization.name %></h1>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Extension</th>
  </tr>

<% @extensions.each do |extension| %>
  <tr>
    <td><%=h extension.user.login unless extension.user.nil?%></td>
    <td><%=h extension.user.email unless extension.user.nil? %></td>
    <td><%= link_to("#{extension.number}", extension_path(extension)) %></td>
  </tr>
<% end %>
</table>

<br />
